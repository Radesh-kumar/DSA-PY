    "def reverse(arr):\n",
    "    i = 0\n",
    "    j = len(arr) - 1\n",
    "    while(i < j):\n",
    "        arr[i], arr[j] = arr[j], arr[i]\n",
    "        i += 1\n",
    "        j -= 1\n",
    "    return arr"
   ]
  },
